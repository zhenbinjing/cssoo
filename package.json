{
  "name": "i-cut",
  "version": "2.22.0",
  "description": "Personal technical style and development direction",
  "main": "gulpfile.js",
  "scripts": {
    "gulp": "gulp default --color && gulp online --color",
    "dev": "webpack-dev-server --inline --colors --config ./build/webpack.config.dev.js",
    "clean": "node ./build/clean.js",
    "build": "cross-env MODERN=modern npm run build:clever && cross-env MDLEGACY=mdlegacy npm run build:clever",
    "build:clever": "npm run clean && cross-env NODE_ENV=production node ./build/build.js",
    "build:legacy": "npm run clean && cross-env NODE_ENV=production LEGACY=legacy node ./build/build.js",
    "start:prod": "cross-env NODE_ENV=production node server",
    "ssr": "cross-env MODERN=modern npm run build:client && cross-env MDLEGACY=mdlegacy npm run build:client && npm run build:server && npm run start:prod",
    "ssr:clever": "npm run build:client && npm run build:server && npm run start:prod",
    "ssr:legacy": "cross-env LEGACY=legacy npm run build:client && npm run build:server && npm run start:prod",
    "build:client": "npm run clean && cross-env NODE_ENV=production VUE_SSR=ssr webpack --config ./build/webpack.config.client.js --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.server.js --hide-modules",
    "lint": "eslint --fix v-src/**/*.{js,vue}",
    "format": "prettier-eslint --write v-src/**/*.js",
    "rmin": "npm run rpts && npm run rpbl",
    "rpts": "rollup -c ./build/rollup.config.ts.js",
    "rpbl": "rollup -c ./build/rollup.config.js",
    "test": "karma start ./build/karma.config.js --single-run",
    "sauce": "karma start ./build/karma.config.sauce.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhenbinjing/i-Cut.git"
  },
  "keywords": [
    "rem",
    "adaptive",
    "responsive",
    "flex",
    "flexbox",
    "flexgrid",
    "grid",
    "gulp",
    "rollup",
    "webpack"
  ],
  "author": "zhenbinjing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zhenbinjing/i-Cut/issues"
  },
  "homepage": "https://github.com/zhenbinjing/i-Cut#readme",
